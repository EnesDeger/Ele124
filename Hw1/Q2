#include <stdio.h>


int
main(void) 

{ 

int integer1; /*there must be a ; */
int integer2; /*there must be a ; */
int x;


printf( "Enter first integer\n" ); /* there was no "" here */

scanf( "%d", &integer1 );


printf( "Enter second integer\n" ); 

scanf( "%d", &integer2 );



x = integer1 + integer2; /* "sum" is an undefined variable. So it is changed with "x" which is defined before */


printf( "Sum is %d\n", x ); /* again about "sum" */

return (0); /* 0 should be in parentheses, but in this situation there is no problem. */


}
