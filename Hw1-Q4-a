#include <stdio.h>

// Fonksiyon prototipi
int computeHomeValue(int popularity, int size);

int main() {
    int popularity, size, homeValue;

    // Kullanıcıdan giriş al
    printf("Enter popularity: ");
    scanf("%d", &popularity);

    printf("Enter size: ");
    scanf("%d", &size);

    // Fonksiyonu çağır ve sonucu yazdır
    homeValue = computeHomeValue(popularity, size);
    printf("Home value is: %d\n", homeValue);

    return 0;
}

// Konut değerini hesaplayan fonksiyon
int computeHomeValue(int popularity, int size) {
    return (popularity * popularity * popularity + size * size) * 10000;
}